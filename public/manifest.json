{
  "manifest_version": 3,
  "name": "Amazon Subscribe & Save one-click cancel",
  "description": "Easy way to cancel Subscribe & Save subscriptions on Amazon with just one button click",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://www.amazon.com/auto-deliveries/*",
        "https://www.amazon.com.au/auto-deliveries/*",
        "https://www.amazon.com/gp/subscribe-and-save/manager/viewsubscriptions",
        "https://www.amazon.com.au/gp/subscribe-and-save/manager/viewsubscriptions"
      ],
      "js": ["js/content_script.js"]
    }
  ]
}
